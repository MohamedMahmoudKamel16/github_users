{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "no-console": 1,
        "global-require": 0,
        "jsx-a11y/href-no-hash": 0,
        "one-var": "off",
        "max-len": "off"
    }
}
